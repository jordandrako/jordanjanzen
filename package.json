{
  "name": "jordanjanzen.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "yarn checkenv",
    "start": "react-scripts-ts start",
    "prebuild": "yarn checkenv",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "checkenv": "node scripts/check-env.js",
    "branchcheck": "git-branch-is master",
    "predeploy":
      "yarn branchcheck && cross-env REACT_APP_DATABASE=production yarn build",
    "prestaging": "yarn build",
    "deploy":
      "aws s3 sync build/ s3://www.jordanjanzen.com --cache-control max-age=172800",
    "staging":
      "aws s3 sync build/ s3://staging.jordanjanzen.com --cache-control max-age=180",
    "postdeploy":
      "aws cloudfront create-invalidation --distribution-id E1SASUQHSLUTW3 --paths /index.html",
    "poststaging":
      "aws cloudfront create-invalidation --distribution-id E2R8NA2ILW4G96 --paths /index.html"
  },
  "dependencies": {
    "check-env": "^1.3.0",
    "dotenv": "^5.0.1",
    "firebase": "^4.12.1",
    "moment": "^2.22.0",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "re-base": "^3.2.2",
    "react": "^16.3.2",
    "react-autosize-textarea": "^3.0.3",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.3.2",
    "react-dropzone": "^4.2.9",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.5.0",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.15.1",
    "react-transition-group": "^2.3.1",
    "sha1": "^1.1.1",
    "styled-components": "^3.2.6",
    "styled-normalize": "^4.0.0",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.2.0",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.0",
    "@types/react": "^16.3.13",
    "@types/react-document-title": "^2.0.2",
    "@types/react-dom": "^16.0.5",
    "@types/react-loadable": "^5.3.4",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-transition-group": "^2.0.9",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "git-branch-is": "^0.1.0",
    "prettier": "^1.12.1",
    "prettier-tslint": "^0.4.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3"
  }
}
